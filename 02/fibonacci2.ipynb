   "source": [
    "memo = {1: 1, 2: 1}\n",
    "\n",
    "\n",
    "def fibonacci(n):\n",
    "    if n in memo:\n",
    "        return memo[n]\n",
    "\n",
    "    memo[n] = fibonacci(n - 2) + fibonacci(n - 1)\n",
    "\n",
    "    return memo[n]"
   ]

   "source": [
    "def hanoi(n, src, dist, via):\n",
    "    if n > 1:\n",
    "        hanoi(n - 1, src, via, dist)\n",
    "        print(src + ' -> ' + dist)\n",
    "        hanoi(n - 1, via, dist, src)\n",
    "    else:\n",
    "        print(src + ' -> ' + dist)\n",
    "\n"
   ]
  },

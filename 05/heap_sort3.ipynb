   "source": [
    "import heapq\n",
    "\n",
    "def heap_sort(array):\n",
    "    h = array.copy()\n",
    "    heapq.heapify(h)\n",
    "\n",
    "    return [heapq.heappop(h) for _ in range(len(array))]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[2, 4, 5, 6, 7, 8, 9, 11, 13, 15]\n"
     ]
    }
   ],
   "source": [
    "data = [6, 15, 4, 2, 8, 5, 11, 9, 7, 13]\n",
    "\n",
    "print(heap_sort(data))"
   ]
  }
